---
to: <%= app %>/src/reducers/<%= h.inflection.dasherize(name) %>/<%= h.inflection.dasherize(name) %>.reducer.js
---
<%
  typeName = name.toUpperCase()
  camlName = h.inflection.camelize(name, true)
%>
const init = {};

export const <%= camlName %> = (state = init, action) => {
  switch (action.type) {

    // case 'SET_<%= typeName %>':
    //   return [...action.<%= camlName %>]
    //   return {...action.<%= camlName %>}

    // case 'UPDATE_<%= typeName %>':
    //   return [...state, ...action.<%= camlName %>]
    //   return {...state, ...action.<%= camlName %>}

    case 'RESET':
    case 'RESET_<%= typeName %>':
      return init;

    default:
      return state;
  }
}
